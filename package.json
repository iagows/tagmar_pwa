{
	"name": "tagmar_pwa",
	"private": true,
	"version": "0.2.11",
	"type": "module",
	"homepage": "https://github.com/iagows/tagmar_pwa",
	"scripts": {
		"dev": "bunx --bun vite",
		"build": "tsc && bunx --bun vite build",
		"buildMinor": "bun run incMinor && bun run build",
		"buildMajor": "bun run incMajor && bun run build",
		"buildPatch": "bun run incPatch && bun run build",
		"lint": "npx @biomejs/biome check --apply .",
		"preview": "bunx --bun vite preview",
		"predeploy": "npm run build",
		"gen-assets": "pwa-assets-generator",
		"deploy": "gh-pages -d dist",
		"incPatch": "bun predeploy.ts p",
		"incMinor": "bun predeploy.ts m",
		"incMajor": "bun predeploy.ts M"
	},
	"peerDependencies": {
		"react": "^17.0.0 || ^18.0.0",
		"react-dom": "^17.0.0 || ^18.0.0"
	},
	"dependencies": {
		"@3d-dice/dice-box-threejs": "^0.0.12",
		"@emotion/react": "^11.11.4",
		"@emotion/styled": "^11.11.5",
		"@fontsource/poppins": "^5.0.13",
		"@mui/icons-material": "^5.15.15",
		"@mui/material": "^5.15.15",
		"@react-native-async-storage/async-storage": "^1.23.1",
		"@react-oauth/google": "^0.12.1",
		"@reduxjs/toolkit": "^2.2.3",
		"axios": "^1.6.8",
		"date-fns": "^3.6.0",
		"date-fns-tz": "^3.1.3",
		"diacritics": "^1.3.0",
		"nanoid": "^5.0.7",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^9.1.1",
		"react-router-dom": "^6.22.3",
		"redux-persist": "^6.0.0",
		"sharp": "^0.33.3",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@biomejs/biome": "1.7.0",
		"@types/diacritics": "^1.3.3",
		"@types/react": "^18.2.79",
		"@types/react-dom": "^18.2.25",
		"@vite-pwa/assets-generator": "^0.2.4",
		"@vitejs/plugin-react": "^4.2.1",
		"gh-pages": "^6.1.1",
		"typescript": "^5.4.5",
		"vite": "^5.2.9",
		"vite-plugin-pwa": "^0.19.8",
		"vite-plugin-svgr": "^4.2.0"
	}
